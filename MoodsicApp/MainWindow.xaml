<Window x:Class="MoodsicApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MoodsicApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TabControl Name="tabs">
            <TabItem Name="tabCamera" Header="Camera">
                <Label Content="Select an image" Height="100" Width="100"/>
            </TabItem>
            <TabItem Header="Result"></TabItem>
            <TabItem Header="Console"></TabItem>
        </TabControl>
    </Grid>
</Window>
